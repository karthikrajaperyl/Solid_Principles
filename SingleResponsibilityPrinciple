class Employee
{
    var id:Int;
    var name:String;
    var position:String;
    init(id:Int,name:String,position:String)
    {
        self.id=id
        self.name=name;
        self.position=position
    }
    func display()
    {
     print("Employee name is \(name) with id \(id) with position \(position)")   
    }
    
}
let employee1=Employee(id:1,name:"karthik",position:"Software developer")
let employee2=Employee(id:2,name:"Raja",position:"Senior Software developer")
employee1.display()
employee2.display()
